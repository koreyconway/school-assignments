 433C            ; Lcd Routines
 433C            ;
 433C            ; Write a byte to the LCD Data Register
 433C            LCD2PP_Data:
 433C  1C024004        bset  PTT,LCD_RS     ; select lcd data buffer RS=1
 4340  16437D          jsr   LCD_W_8        ; write byte
 4343  3D              rts
 4344            
 4344            ; Write a byte to the LCD Instruction Register (leaves LCD in Data mode)
 4344            LD2PP_Instruction:
 4344  1D024004          bclr   PTT,LCD_RS        ; select lcd command buffer
 4348  16437D            jsr    LCD_W_8           ; wait
 434B  1C024004          bset   PTT,LCD_RS        ; select data buffer
 434F  3D                rts
