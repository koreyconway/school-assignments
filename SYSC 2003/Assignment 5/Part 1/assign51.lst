 438A            ; Delay routines
 438A            ;
 438A            ;
 438A            ; Generate a 50 ms delay
 438A            DELAY50M:
 438A  34                  pshx
 438B  CEC34E              ldx  #49998      ; delay 50,000 usecs,
 438E  1643AA              jsr  DELML01     ; call usec delay
 4391  30                  pulx
 4392  3D                  rts
 4393            ;
 4393            ;
 4393            ; Generate a 20 ms delay
 4393            DELAY20M:
 4393  0703                bsr  DELAY10M
 4395  0701                bsr  DELAY10M 
 4397  3D                  rts
 4398            ;
 4398            ; Generate a 10 ms delay
 4398            DELAY10M:                            ; jsr=4cyles
 4398  34                  pshx             ; 2 cycles ,save x
 4399  CE270E              ldx  #9998       ; 2 cycles,delay 9998 usec + 2 for this routine
 439C  1643AA              jsr  DELML01     ; call usec delay, this delay offset in sub
 439F  30                  pulx             ; 3 cycles restore x
 43A0  3D                  rts              ; 5 cycles
 43A1            ;
 43A1            ;
 43A1            ; Generate a 1 ms delay
 43A1            DELAY1MS:
 43A1                                       ; jsr=4cyles
 43A1  34                  pshx             ; 2 cycles ,save x
 43A2  CE03E6              ldx  #998       ; 2 cycles,delay 9998 usec + 2 for this routine
 43A5  1643AA              jsr  DELML01     ; call usec delay, this delay offset in sub
 43A8  30                  pulx             ; 3 cycles restore x
 43A9  3D                  rts              ; 5 cycles
 43AA            
 43AA            
 43AA            ;
 43AA            ; 8 cycles = 1 usec e = 8mhz
 43AA            DELML01:
 43AA  A7                  nop              ; 1 cycle
 43AB  A7                  nop              ; 1 cycle
 43AC  A7                  nop              ; 1 cycle
 43AD  A7                  nop              ; 1 cycle
 43AE  09                  dex              ; 1 cycle 
 43AF  26F9                bne   DELML01    ; 3 cycles
 43B1  3D                  rts              ; 5 cycles
