 42EC                      
 42EC            Lcd2PP_Init:	; Note : Use 4-bit init sequence (not 8-bit)  Page 3 LCD_spec.pdf
 42EC            		; Bottom table contains sequence of instructions
 42EC            		; Each row in the table represents one WRITE to the LCD instruction register (via Port P)
 42EC            		;	First instruction involves only a 4-bit instruction (one WRITE)
 42EC            		;	Following instructions involve 8 bit instruction, therefore
 42EC            		;		2 * 4-bit writes
 42EC            
 42EC            	; "System init"
 42EC            	; Setup Port T for output
 42EC  180B0F0242           movb #$0F,DDRT        ; setup port T
 42F1  180B000240           movb #$00,PTT         ; all low
 42F6                    ; Disable SPI AND setup SPI1 as four output bits
 42F6  1C025A0F                bset  DDRP,#$0F           ; set P0-3 as outputs
 42FA  1D00F040            bclr  SPI1CR1,#$40    ; Disable SP by turning SPI1 off
 42FE            
 42FE  180BFE0252           movb #$FE,DDRM        ; set PM1-7 as outputs
 4303  180B000250           movb #$00,PTM         ; D.P.(PM2) = Off, U7_EN(PM3)= low,
 4308                                            ; SS0*(PM4), SS1*(PM5), SS2*(PM6) = Low
 4308                                            ; Heat(PM7) = Off
 4308            
 4308  1D02400E            bclr    PTT,LCD_RW+LCD_RS+LCD_EN  ; select lcd commands Cs=0 En=0
 430C            
 430C  16438A              jsr      DELAY50M
 430F  8602                ldaa     #$02         ; Set to 4-bit operation (0010)
 4311  164350              jsr      LCD2PP_4     ; This first instruction is only 4 bits long!!!  Rest are 8 bits.  
 4314  16438A              jsr      DELAY50M
 4317            
 4317                    ;  ldaa     #$2c                ; Function Set = 001(D/L)NF** where D/L = 0(4-bit) N=1(2-lines) F=0(font=5x7 dots)
 4317  8628                ldaa     #$28         ; Function Set = 001(D/L)NF** where D/L = 0(4-bit) N=1(2-lines) F=0(font=5x7 dots)
 4319  164344              jsr      LD2PP_Instruction         
 431C  164398              jsr      DELAY10M         
 431F            
 431F  860E                ldaa      #$0e        ; Display On/off Control = 00001DCB where D=1(display on) C=1(cursor on) B=0 (blink off)
 4321  164344              jsr      LD2PP_Instruction          
 4324  164398              jsr      DELAY10M          
 4327                            
 4327  8601                ldaa     #$01         ; Clear display = 00000001
 4329  164344              jsr      LD2PP_Instruction           
 432C  164393              jsr      DELAY20M          
