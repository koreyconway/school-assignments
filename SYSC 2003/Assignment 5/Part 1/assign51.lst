 4350            LCD2PP_4:			; Destroys a and b
 4350  1C024880           bset   PTS,#U21_EN     ; set U21_EN high so that latch becomes transparent
 4354  1643A1             jsr      DELAY1MS      ; delay     
 4357  F60258             ldab     PTP              ; Port P
 435A  C4F0               andb     #$f0             ; get only bits 4 - 7
 435C  840F               anda     #$0f             ; get data
 435E  1806               aba
 4360  7A0258             staa     PTP              ; save data 
 4363                    ; For LCD's write cycle, Enable must pulse high and then low (for specified time)
 4363  1D024008           bclr     PTT,LCD_EN       ; enable low
 4367  1643A1             jsr      DELAY1MS         ; delay for LCD
 436A  1C024008           bset     PTT,LCD_EN       ; latch data
 436E  1643A1             jsr      DELAY1MS         ; delay for LCD 
 4371  1D024008           bclr     PTT,LCD_EN           ; enable low
 4375  1643A1             jsr      DELAY1MS
 4378  1D024880           bclr  PTS,#U21_EN    ; set U21_EN low to isolate LCD from parallel control (outputs are latched)
 437C  3D                 rts
