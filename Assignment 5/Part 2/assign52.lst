 445D            Lcd2PP_Init:	; Note : Use 4-bit init sequence (not 8-bit)  Page 3 LCD_spec.pdf
 445D            		; Bottom table contains sequence of instructions
 445D            		; Each row in the table represents one WRITE to the LCD instruction register (via Port P)
 445D            		;	First instruction involves only a 4-bit instruction (one WRITE)
 445D            		;	Following instructions involve 8 bit instruction, therefore
 445D            		;		2 * 4-bit writes
 445D            
 445D            	; "System init"
 445D            	; Setup Port T for output
 445D  180B0F0242           movb #$0F,DDRT        ; setup port T
 4462  180B000240           movb #$00,PTT         ; all low
 4467                    ; Disable SPI AND setup SPI1 as four output bits
 4467  1C025A0F                bset  DDRP,#$0F           ; set P0-3 as outputs
 446B  1D00F040            bclr  SPI1CR1,#$40    ; Disable SP by turning SPI1 off
 446F            
 446F  180BFE0252           movb #$FE,DDRM        ; set PM1-7 as outputs
 4474  180B000250           movb #$00,PTM         ; D.P.(PM2) = Off, U7_EN(PM3)= low,
 4479                                            ; SS0*(PM4), SS1*(PM5), SS2*(PM6) = Low
 4479                                            ; Heat(PM7) = Off
 4479            
 4479  1D02400E            bclr    PTT,LCD_RW+LCD_RS+LCD_EN  ; select lcd commands Cs=0 En=0
 447D            
 447D  1644FB              jsr      DELAY50M
 4480  8602                ldaa     #$02         ; Set to 4-bit operation (0010)
 4482  1644C1              jsr      LCD2PP_4     ; This first instruction is only 4 bits long!!!  Rest are 8 bits.  
 4485  1644FB              jsr      DELAY50M
 4488            
 4488                    ;  ldaa     #$2c                ; Function Set = 001(D/L)NF** where D/L = 0(4-bit) N=1(2-lines) F=0(font=5x7 dots)
 4488  8628                ldaa     #$28         ; Function Set = 001(D/L)NF** where D/L = 0(4-bit) N=1(2-lines) F=0(font=5x7 dots)
 448A  1644B5              jsr      LD2PP_Instruction         
 448D  164509              jsr      DELAY10M         
 4490            
 4490  860E                ldaa      #$0e        ; Display On/off Control = 00001DCB where D=1(display on) C=1(cursor on) B=0 (blink off)
 4492  1644B5              jsr      LD2PP_Instruction          
 4495  164509              jsr      DELAY10M          
 4498                            
 4498  8601                ldaa     #$01         ; Clear display = 00000001
 449A  1644B5              jsr      LD2PP_Instruction           
 449D  164504              jsr      DELAY20M          
 44A0  8680                ldaa     #$80         ; DDRAM Address Set = 01xxxxxx where xxxxxx = address
 44A2  1644B5              jsr      LD2PP_Instruction
 44A5  164509              jsr      DELAY10M        
 44A8            
 44A8            ; Reset Lcd states to rest
 44A8  1D02400E           bclr    PTT,LCD_RS+LCD_RW+LCD_EN ; turn all signals off on lcd
 44AC  3D                  rts
 44AD            ;
 44AD            ;-----------------------------------------------
 44AD            ; Lcd Routines
 44AD            ;
 44AD            ; Write a byte to the LCD Data Register
 44AD            LCD2PP_Data:
 44AD  1C024004        bset  PTT,LCD_RS     ; select lcd data buffer RS=1
 44B1  1644EE          jsr   LCD_W_8        ; write byte
 44B4  3D              rts
 44B5            
 44B5            ; Write a byte to the LCD Instruction Register (leaves LCD in Data mode)
 44B5            LD2PP_Instruction:
 44B5  1D024004          bclr   PTT,LCD_RS        ; select lcd command buffer
 44B9  1644EE            jsr    LCD_W_8           ; wait
 44BC  1C024004          bset   PTT,LCD_RS        ; select data buffer
 44C0  3D                rts
 44C1            
 44C1            LCD2PP_4:			; Destroys a and b
 44C1  1C024880           bset   PTS,#U21_EN     ; set U21_EN high so that latch becomes transparent
 44C5  164512             jsr      DELAY1MS      ; delay     
 44C8  F60258             ldab     PTP              ; Port P
 44CB  C4F0               andb     #$f0             ; get only bits 4 - 7
 44CD  840F               anda     #$0f             ; get data
 44CF  1806               aba
 44D1  7A0258             staa     PTP              ; save data 
 44D4                    ; For LCD's write cycle, Enable must pulse high and then low (for specified time)
 44D4  1D024008           bclr     PTT,LCD_EN       ; enable low
 44D8  164512             jsr      DELAY1MS         ; delay for LCD
 44DB  1C024008           bset     PTT,LCD_EN       ; latch data
 44DF  164512             jsr      DELAY1MS         ; delay for LCD 
 44E2  1D024008           bclr     PTT,LCD_EN           ; enable low
 44E6  164512             jsr      DELAY1MS
 44E9  1D024880           bclr  PTS,#U21_EN    ; set U21_EN low to isolate LCD from parallel control (outputs are latched)
 44ED  3D                 rts
 44EE            ;
 44EE            ;
 44EE            ; Lcd Write 8 bit Data , lower 4 bits first in acc A   (Destroys A)
 44EE            LCD_W_8:					
 44EE  36                 psha                     ; save a 
 44EF  44                 lsra                     ; shift upper 4 bits to lower
 44F0  44                 lsra
 44F1  44                 lsra
 44F2  44                 lsra
 44F3  1644C1             jsr      LCD2PP_4        ; write upper 4 bits to lcd
 44F6  32                 pula
 44F7  1644C1             jsr      LCD2PP_4         ; write lower 4 bits to lcd
 44FA  3D                 rts
