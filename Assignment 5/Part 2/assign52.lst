 451D                      
 451D            Lcd2PP_Init:	; Note : Use 4-bit init sequence (not 8-bit)  Page 3 LCD_spec.pdf
 451D            		; Bottom table contains sequence of instructions
 451D            		; Each row in the table represents one WRITE to the LCD instruction register (via Port P)
 451D            		;	First instruction involves only a 4-bit instruction (one WRITE)
 451D            		;	Following instructions involve 8 bit instruction, therefore
 451D            		;		2 * 4-bit writes
 451D            
 451D            	; "System init"
 451D            	; Setup Port T for output
 451D  180B0F0242           movb #$0F,DDRT        ; setup port T
 4522  180B000240           movb #$00,PTT         ; all low
 4527                    ; Disable SPI AND setup SPI1 as four output bits
 4527  1C025A0F                bset  DDRP,#$0F           ; set P0-3 as outputs
 452B  1D00F040            bclr  SPI1CR1,#$40    ; Disable SP by turning SPI1 off
 452F            
 452F  180BFE0252           movb #$FE,DDRM        ; set PM1-7 as outputs
 4534  180B000250           movb #$00,PTM         ; D.P.(PM2) = Off, U7_EN(PM3)= low,
 4539                                            ; SS0*(PM4), SS1*(PM5), SS2*(PM6) = Low
 4539                                            ; Heat(PM7) = Off
 4539            
 4539  1D02400E            bclr    PTT,LCD_RW+LCD_RS+LCD_EN  ; select lcd commands Cs=0 En=0
 453D            
 453D  1645BB              jsr      DELAY50M
 4540  8602                ldaa     #$02         ; Set to 4-bit operation (0010)
 4542  164581              jsr      LCD2PP_4     ; This first instruction is only 4 bits long!!!  Rest are 8 bits.  
 4545  1645BB              jsr      DELAY50M
 4548            
 4548                    ;  ldaa     #$2c                ; Function Set = 001(D/L)NF** where D/L = 0(4-bit) N=1(2-lines) F=0(font=5x7 dots)
 4548  8628                ldaa     #$28         ; Function Set = 001(D/L)NF** where D/L = 0(4-bit) N=1(2-lines) F=0(font=5x7 dots)
 454A  164575              jsr      LD2PP_Instruction         
 454D  1645C9              jsr      DELAY10M         
 4550            
 4550  860E                ldaa      #$0e        ; Display On/off Control = 00001DCB where D=1(display on) C=1(cursor on) B=0 (blink off)
 4552  164575              jsr      LD2PP_Instruction          
 4555  1645C9              jsr      DELAY10M          
 4558                            
 4558  8601                ldaa     #$01         ; Clear display = 00000001
 455A  164575              jsr      LD2PP_Instruction           
 455D  1645C4              jsr      DELAY20M          
 4560  8680                ldaa     #$80         ; DDRAM Address Set = 01xxxxxx where xxxxxx = address
 4562  164575              jsr      LD2PP_Instruction
 4565  1645C9              jsr      DELAY10M        
