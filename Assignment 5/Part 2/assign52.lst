 45BB            ;
 45BB            ; Delay routines
 45BB            ;
 45BB            ;
 45BB            ; Generate a 50 ms delay
 45BB            DELAY50M:
 45BB  34                  pshx
 45BC  CEC34E              ldx  #49998      ; delay 50,000 usecs,
 45BF  1645DB              jsr  DELML01     ; call usec delay
 45C2  30                  pulx
 45C3  3D                  rts
 45C4            ;
 45C4            ;
 45C4            ; Generate a 20 ms delay
 45C4            DELAY20M:
 45C4  0703                bsr  DELAY10M
 45C6  0701                bsr  DELAY10M 
 45C8  3D                  rts
 45C9            ;
 45C9            ; Generate a 10 ms delay
 45C9            DELAY10M:                            ; jsr=4cyles
 45C9  34                  pshx             ; 2 cycles ,save x
 45CA  CE270E              ldx  #9998       ; 2 cycles,delay 9998 usec + 2 for this routine
 45CD  1645DB              jsr  DELML01     ; call usec delay, this delay offset in sub
 45D0  30                  pulx             ; 3 cycles restore x
 45D1  3D                  rts              ; 5 cycles
 45D2            ;
 45D2            ;
 45D2            ; Generate a 1 ms delay
 45D2            DELAY1MS:
 45D2                                       ; jsr=4cyles
 45D2  34                  pshx             ; 2 cycles ,save x
 45D3  CE03E6              ldx  #998       ; 2 cycles,delay 9998 usec + 2 for this routine
 45D6  1645DB              jsr  DELML01     ; call usec delay, this delay offset in sub
 45D9  30                  pulx             ; 3 cycles restore x
 45DA  3D                  rts              ; 5 cycles
 45DB            
 45DB            
 45DB            ;
 45DB            ; 8 cycles = 1 usec e = 8mhz
 45DB            DELML01:
 45DB  A7                  nop              ; 1 cycle
 45DC  A7                  nop              ; 1 cycle
 45DD  A7                  nop              ; 1 cycle
 45DE  A7                  nop              ; 1 cycle
 45DF  09                  dex              ; 1 cycle 
 45E0  26F9                bne   DELML01    ; 3 cycles
 45E2  3D                  rts              ; 5 cycles
