 44FB            ;
 44FB            ; Delay routines
 44FB            ;
 44FB            ;
 44FB            ; Generate a 50 ms delay
 44FB            DELAY50M:
 44FB  34                  pshx
 44FC  CEC34E              ldx  #49998      ; delay 50,000 usecs,
 44FF  16451B              jsr  DELML01     ; call usec delay
 4502  30                  pulx
 4503  3D                  rts
 4504            ;
 4504            ;
 4504            ; Generate a 20 ms delay
 4504            DELAY20M:
 4504  0703                bsr  DELAY10M
 4506  0701                bsr  DELAY10M 
 4508  3D                  rts
 4509            ;
 4509            ; Generate a 10 ms delay
 4509            DELAY10M:                            ; jsr=4cyles
 4509  34                  pshx             ; 2 cycles ,save x
 450A  CE270E              ldx  #9998       ; 2 cycles,delay 9998 usec + 2 for this routine
 450D  16451B              jsr  DELML01     ; call usec delay, this delay offset in sub
 4510  30                  pulx             ; 3 cycles restore x
 4511  3D                  rts              ; 5 cycles
 4512            ;
 4512            ;
 4512            ; Generate a 1 ms delay
 4512            DELAY1MS:
 4512                                       ; jsr=4cyles
 4512  34                  pshx             ; 2 cycles ,save x
 4513  CE03E6              ldx  #998       ; 2 cycles,delay 9998 usec + 2 for this routine
 4516  16451B              jsr  DELML01     ; call usec delay, this delay offset in sub
 4519  30                  pulx             ; 3 cycles restore x
 451A  3D                  rts              ; 5 cycles
 451B            
 451B            
 451B            ;
 451B            ; 8 cycles = 1 usec e = 8mhz
 451B            DELML01:
 451B  A7                  nop              ; 1 cycle
 451C  A7                  nop              ; 1 cycle
 451D  A7                  nop              ; 1 cycle
 451E  A7                  nop              ; 1 cycle
 451F  09                  dex              ; 1 cycle 
 4520  26F9                bne   DELML01    ; 3 cycles
 4522  3D                  rts              ; 5 cycles
