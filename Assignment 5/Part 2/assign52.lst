 456D            ;-----------------------------------------------
 456D            ; Lcd Routines
 456D            ;
 456D            ; Write a byte to the LCD Data Register
 456D            LCD2PP_Data:
 456D  1C024004        bset  PTT,LCD_RS     ; select lcd data buffer RS=1
 4571  1645AE          jsr   LCD_W_8        ; write byte
 4574  3D              rts
 4575            
 4575            ; Write a byte to the LCD Instruction Register (leaves LCD in Data mode)
 4575            LD2PP_Instruction:
 4575  1D024004          bclr   PTT,LCD_RS        ; select lcd command buffer
 4579  1645AE            jsr    LCD_W_8           ; wait
 457C  1C024004          bset   PTT,LCD_RS        ; select data buffer
 4580  3D                rts
 4581            
 4581            LCD2PP_4:			; Destroys a and b
 4581  1C024880           bset   PTS,#U21_EN     ; set U21_EN high so that latch becomes transparent
 4585  1645D2             jsr      DELAY1MS      ; delay     
 4588  F60258             ldab     PTP              ; Port P
 458B  C4F0               andb     #$f0             ; get only bits 4 - 7
 458D  840F               anda     #$0f             ; get data
 458F  1806               aba
 4591  7A0258             staa     PTP              ; save data 
 4594                    ; For LCD's write cycle, Enable must pulse high and then low (for specified time)
 4594  1D024008           bclr     PTT,LCD_EN       ; enable low
 4598  1645D2             jsr      DELAY1MS         ; delay for LCD
 459B  1C024008           bset     PTT,LCD_EN       ; latch data
 459F  1645D2             jsr      DELAY1MS         ; delay for LCD 
 45A2  1D024008           bclr     PTT,LCD_EN           ; enable low
 45A6  1645D2             jsr      DELAY1MS
 45A9  1D024880           bclr  PTS,#U21_EN    ; set U21_EN low to isolate LCD from parallel control (outputs are latched)
 45AD  3D                 rts
 45AE            ;
 45AE            ;
 45AE            ; Lcd Write 8 bit Data , lower 4 bits first in acc A   (Destroys A)
 45AE            LCD_W_8:					
 45AE  36                 psha                     ; save a 
 45AF  44                 lsra                     ; shift upper 4 bits to lower
 45B0  44                 lsra
 45B1  44                 lsra
 45B2  44                 lsra
 45B3  164581             jsr      LCD2PP_4        ; write upper 4 bits to lcd
 45B6  32                 pula
 45B7  164581             jsr      LCD2PP_4         ; write lower 4 bits to lcd
 45BA  3D                 rts
