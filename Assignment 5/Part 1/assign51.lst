 42F5            Lcd2PP_Init:	; Note : Use 4-bit init sequence (not 8-bit)  Page 3 LCD_spec.pdf
 42F5            		; Bottom table contains sequence of instructions
 42F5            		; Each row in the table represents one WRITE to the LCD instruction register (via Port P)
 42F5            		;	First instruction involves only a 4-bit instruction (one WRITE)
 42F5            		;	Following instructions involve 8 bit instruction, therefore
 42F5            		;		2 * 4-bit writes
 42F5            
 42F5            	; "System init"
 42F5            	; Setup Port T for output
 42F5  180B0F0242           movb #$0F,DDRT        ; setup port T
 42FA  180B000240           movb #$00,PTT         ; all low
 42FF                    ; Disable SPI AND setup SPI1 as four output bits
 42FF  1C025A0F                bset  DDRP,#$0F           ; set P0-3 as outputs
 4303  1D00F040            bclr  SPI1CR1,#$40    ; Disable SP by turning SPI1 off
 4307            
 4307  180BFE0252           movb #$FE,DDRM        ; set PM1-7 as outputs
 430C  180B000250           movb #$00,PTM         ; D.P.(PM2) = Off, U7_EN(PM3)= low,
 4311                                            ; SS0*(PM4), SS1*(PM5), SS2*(PM6) = Low
 4311                                            ; Heat(PM7) = Off
 4311            
 4311  1D02400E            bclr    PTT,LCD_RW+LCD_RS+LCD_EN  ; select lcd commands Cs=0 En=0
 4315            
 4315  164393              jsr      DELAY50M
 4318  8602                ldaa     #$02         ; Set to 4-bit operation (0010)
 431A  164359              jsr      LCD2PP_4     ; This first instruction is only 4 bits long!!!  Rest are 8 bits.  
 431D  164393              jsr      DELAY50M
 4320            
 4320                    ;  ldaa     #$2c                ; Function Set = 001(D/L)NF** where D/L = 0(4-bit) N=1(2-lines) F=0(font=5x7 dots)
 4320  8628                ldaa     #$28         ; Function Set = 001(D/L)NF** where D/L = 0(4-bit) N=1(2-lines) F=0(font=5x7 dots)
 4322  16434D              jsr      LD2PP_Instruction         
 4325  1643A1              jsr      DELAY10M         
 4328            
 4328  860E                ldaa      #$0e        ; Display On/off Control = 00001DCB where D=1(display on) C=1(cursor on) B=0 (blink off)
 432A  16434D              jsr      LD2PP_Instruction          
 432D  1643A1              jsr      DELAY10M          
