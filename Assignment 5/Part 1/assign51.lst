 4359            LCD2PP_4:			; Destroys a and b
 4359  1C024880           bset   PTS,#U21_EN     ; set U21_EN high so that latch becomes transparent
 435D  1643AA             jsr      DELAY1MS      ; delay     
 4360  F60258             ldab     PTP              ; Port P
 4363  C4F0               andb     #$f0             ; get only bits 4 - 7
 4365  840F               anda     #$0f             ; get data
 4367  1806               aba
 4369  7A0258             staa     PTP              ; save data 
 436C                    ; For LCD's write cycle, Enable must pulse high and then low (for specified time)
 436C  1D024008           bclr     PTT,LCD_EN       ; enable low
 4370  1643AA             jsr      DELAY1MS         ; delay for LCD
 4373  1C024008           bset     PTT,LCD_EN       ; latch data
 4377  1643AA             jsr      DELAY1MS         ; delay for LCD 
 437A  1D024008           bclr     PTT,LCD_EN           ; enable low
 437E  1643AA             jsr      DELAY1MS
 4381  1D024880           bclr  PTS,#U21_EN    ; set U21_EN low to isolate LCD from parallel control (outputs are latched)
 4385  3D                 rts
