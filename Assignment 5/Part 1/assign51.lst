 4393            ; Delay routines
 4393            ;
 4393            ;
 4393            ; Generate a 50 ms delay
 4393            DELAY50M:
 4393  34                  pshx
 4394  CEC34E              ldx  #49998      ; delay 50,000 usecs,
 4397  1643B3              jsr  DELML01     ; call usec delay
 439A  30                  pulx
 439B  3D                  rts
 439C            ;
 439C            ;
 439C            ; Generate a 20 ms delay
 439C            DELAY20M:
 439C  0703                bsr  DELAY10M
 439E  0701                bsr  DELAY10M 
 43A0  3D                  rts
 43A1            ;
 43A1            ; Generate a 10 ms delay
 43A1            DELAY10M:                            ; jsr=4cyles
 43A1  34                  pshx             ; 2 cycles ,save x
 43A2  CE270E              ldx  #9998       ; 2 cycles,delay 9998 usec + 2 for this routine
 43A5  1643B3              jsr  DELML01     ; call usec delay, this delay offset in sub
 43A8  30                  pulx             ; 3 cycles restore x
 43A9  3D                  rts              ; 5 cycles
 43AA            ;
 43AA            ;
 43AA            ; Generate a 1 ms delay
 43AA            DELAY1MS:
 43AA                                       ; jsr=4cyles
 43AA  34                  pshx             ; 2 cycles ,save x
 43AB  CE03E6              ldx  #998       ; 2 cycles,delay 9998 usec + 2 for this routine
 43AE  1643B3              jsr  DELML01     ; call usec delay, this delay offset in sub
 43B1  30                  pulx             ; 3 cycles restore x
 43B2  3D                  rts              ; 5 cycles
 43B3            
 43B3            
 43B3            ;
 43B3            ; 8 cycles = 1 usec e = 8mhz
 43B3            DELML01:
 43B3  A7                  nop              ; 1 cycle
 43B4  A7                  nop              ; 1 cycle
 43B5  A7                  nop              ; 1 cycle
 43B6  A7                  nop              ; 1 cycle
 43B7  09                  dex              ; 1 cycle 
 43B8  26F9                bne   DELML01    ; 3 cycles
 43BA  3D                  rts              ; 5 cycles
