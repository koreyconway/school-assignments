 434D            ; Write a byte to the LCD Instruction Register (leaves LCD in Data mode)
 434D            LD2PP_Instruction:
 434D  1D024004          bclr   PTT,LCD_RS        ; select lcd command buffer
 4351  164386            jsr    LCD_W_8           ; wait
 4354  1C024004          bset   PTT,LCD_RS        ; select data buffer
 4358  3D                rts
