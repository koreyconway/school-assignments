 4345            ; Write a byte to the LCD Data Register
 4345            LCD2PP_Data:
 4345  1C024004        bset  PTT,LCD_RS     ; select lcd data buffer RS=1
 4349  164386          jsr   LCD_W_8        ; write byte
 434C  3D              rts
